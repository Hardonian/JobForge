{
  "name": "@jobforge/autopilot-compat",
  "version": "0.1.0",
  "private": true,
  "description": "Suite-layer compatibility runner for autopilot fixtures",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "autopilot-compat": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm -w tsc -p packages/autopilot-compat/tsconfig.json",
    "dev": "pnpm -w tsc --watch -p packages/autopilot-compat/tsconfig.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "pnpm -w tsc --noEmit -p packages/autopilot-compat/tsconfig.json"
  },
  "dependencies": {
    "@jobforge/autopilot-fixtures": "workspace:*",
    "@jobforge/shared": "workspace:*"
  },
  "devDependencies": {
    "@jobforge/typescript-config": "workspace:*"
  }
}
